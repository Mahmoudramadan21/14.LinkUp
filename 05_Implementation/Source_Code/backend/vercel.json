{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/api-docs",
      "destination": "/api-docs"
    },
    {
      "source": "/api-docs.json",
      "destination": "/api-docs.json"
    },
    {
      "source": "/swagger-ui/(.*)",
      "destination": "/swagger-ui/$1"
    }
  ],
  "headers": [
    {
      "source": "/api-docs",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'self'; script-src 'self'; style-src 'self'; connect-src 'self' https://link-7gq2gvwce-mahmoudramadan21s-projects.vercel.app;"
        }
      ]
    },
    {
      "source": "/swagger-ui/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000"
        }
      ]
    }
  ]
}